{
  "name": "astro-whatsapp-bot",
  "version": "1.0.0",
  "description": "Advanced, comprehensive multi-channel astrology service platform with WhatsApp as the primary channel",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageDirectory=tests/reports/coverage",
    "lint": "eslint src/ tests/ --fix",
    "lint:check": "eslint src/ tests/",
    "format": "prettier --write src/ tests/ docs/",
    "format:check": "prettier --check src/ tests/ docs/",
    "security": "npm audit",
    "security:fix": "npm audit fix",
    "build": "echo \"No build required for Node.js application\"",
    "docs": "jsdoc -c jsdoc.json",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "astrology",
    "whatsapp",
    "bot",
    "ai",
    "personalized",
    "horoscope",
    "kundli"
  ],
  "author": "Astrology Bot Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "stripe": "^14.5.0",
    "twilio": "^4.19.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.2",
    "node-notifier": "^10.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/astro-whatsapp-bot.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/astro-whatsapp-bot/issues"
  },
  "homepage": "https://github.com/your-username/astro-whatsapp-bot#readme"
}
